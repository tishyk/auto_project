pipeline {
    agent 'any'
    stages {
        stage('Test') {
              environment {
                USER_ID = "test_id"
                creds = credentials("$USER_ID")
              }
            steps {
                println("Run tests with credentials: User_ID ${creds_USR} and SECRET_TEXT ${creds_PSW}")
                sh 'python3 --version'
            }
        }       
    }
}
